        "Truthy",
        "Unconfigured",
        "Validators",
        "acquia",
        "bugsnag",
        "certlm",
        "codelyzer",
        "cyclomatic",
        "datepicker",
        "datetime",
        "devkit",
        "devtools",
        "dismissible",
        "dropdown",
        "eofline",
        "fdescribe",
        "jasminewd",
        "jsonc",
        "jsonp",
        "kjhtml",
        "lcovonly",
        "lifecycle",
        "memoized",
        "memoizes",
        "mockserver",
        "ngrx",
        "nospace",
        "nrwl",
        "onespace",
        "personalizations",
        "pickby",
        "polyfill",
        "preallocate",
        "preallocated",
        "prefilled",
        "proxying",
        "quotemark",
        "repo",
        "selectable",
        "serverless",
        "tabindex",
        "timepicker",
        "titlecase",
        "tslib",
        "uikit",
        "unarchived",
        "upsert",
        "userinfo",
        "viewmode"
    ],
    "files.insertFinalNewline": true,
    "html.format.endWithNewline": true,
    "html.format.indentHandlebars": true,
    "html.format.indentInnerHtml": true,
    "html.format.maxPreserveNewLines": 1,
    "html.format.wrapAttributes": "force-expand-multiline",
    "explorer.confirmDragAndDrop": false,
    "prettier.printWidth": 100,
    "cSpell.words": [
        "assignend",
        "assignenter",
        "assignleave",
        "assignover",
        "assignstart",
        "CHOLESTE",
        "Chrom",
        "CHROM",
        "Codegen",
        "coloc",
        "Connnectivity",
        "Extremum",
        "gridster",
        "Highlightable",
        "HMBC",
        "HMQC",
        "horz",
        "HSQC",
        "Initializable",
        "initializables",
        "INMR",
        "Ketcher",
        "lcuvms",
        "Mult",
        "multiplet",
        "multiplets",
        "ngcc",
        "ngneat",
        "noesy",
        "nozone",
        "polylines",
        "Protobuf",
        "ROESY",
        "serie",
        "spectrus",
        "Struct",
        "toastr",
        "TOCSY",
        "transloco",
        "uuidv",
        "uvms",
        "webcomponents",
        "zathras"
    ],
    "prettier.trailingComma": "all",
    "prettier.useEditorConfig": false,
    "editor.defaultFormatter": "esbenp.prettier-vscode",
    "editor.codeActionsOnSave": {
        "source.organizeImports": "never",
        "source.fixAll": "explicit"
    },
    "editor.formatOnSave": true,
    "[typescript]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "[html]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "typescript.preferences.importModuleSpecifier": "non-relative"
}
